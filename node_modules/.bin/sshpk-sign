#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kali/blog_Xelo/Xelo_blog/node_modules/sshpk/bin/node_modules:/home/kali/blog_Xelo/Xelo_blog/node_modules/sshpk/node_modules:/home/kali/blog_Xelo/Xelo_blog/node_modules:/home/kali/blog_Xelo/node_modules:/home/kali/node_modules:/home/node_modules:/node_modules:/home/kali/blog_Xelo/Xelo_blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kali/blog_Xelo/Xelo_blog/node_modules/sshpk/bin/node_modules:/home/kali/blog_Xelo/Xelo_blog/node_modules/sshpk/node_modules:/home/kali/blog_Xelo/Xelo_blog/node_modules:/home/kali/blog_Xelo/node_modules:/home/kali/node_modules:/home/node_modules:/node_modules:/home/kali/blog_Xelo/Xelo_blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sshpk/bin/sshpk-sign" "$@"
else
  exec node  "$basedir/../sshpk/bin/sshpk-sign" "$@"
fi
